/*红包体验金页*/
body{background-color:#f6f6f6;}
.bold{
  font-weight: bold;
}
.header{padding:10px 4% 10px;border-bottom:1px solid #dbdbdb;}
.caution{letter-spacing:.1em;color:#8e8e8e;font-size:1.4em;background-color:#fff;}
.caution>a{float:right;color:#ff4c4c;margin-top:.2em;}
.caution img{vertical-align: middle;margin-right:.2em;margin-top:.1em;}
.priWrap{margin-bottom:1em;}
.topImage{width:92%;margin:0 auto;height:7px;background:url(../images/privilege.png) repeat-x top;}
.tickets{
  position:relative;
  padding:1rem 4%;
  width:92%;
  margin:0 auto;
  color:#808080;
}
.tip_mon{
  color: #8E8E8E;
  padding:1rem 0;
}
.tip_mon>.small-font{
  font-size: 14px;
}
.tip_mon>.bold{
  font-size:16px;
}
.tip_income{
  font-size:1.2em;
  color:#646464;
}
.tip_income>li{
  width: 50%;
  float:left;
  height: 40px;
}
.tickets.used{
  border-bottom:3px solid #dddcdd;
  background:#fff url(../images/used.png) no-repeat 90% center;
  background-size:80px 80px;
}
.rate{
  font-size: 16px;
  padding: 10px 0;
}
.rate-num{
  font-size: 18px;
  font-weight: bold;
}

.tickets.unused{border-bottom:3px solid #ff004f}
.used .tip_income,
.used .tip_mon,
.used .rate{
  color:#dddcdd;
}
.readme{margin-right:.5em;font-size: 15px;}
.fcl{text-align: right;}
.used .fcl{color:#dddcdd}
/*红包页面*/
.tab {
    width: 50%;
    margin: 0 auto;
    border: 1px solid #fff;
    border-radius: 5px;
}
.tab li.current {
    background: #fff;
    color: #ff4c4c;
}
.tab li {
    float: left;
    text-align: center;
    width: 50%;
    color: #fff;
    font-size: .9em;
    padding: .2em 0;
}
.red_package{
  position:relative;
  padding:1rem 4%;
  width:92%;
  margin:0 auto;
  color:#808080;
}
.red_package.used{
  border-bottom:3px solid #dddcdd;
  background:#fff url(../images/red_unuse.png) no-repeat 90% center;
  background-size:70px 70px;
}
.tickets.unused{
  border-bottom:3px solid #ff004f
}
.red_package.out{
  border-bottom:3px solid #dddcdd;
  background:#fff url(../images/red_out.png) no-repeat 90% center;
  background-size:70px 70px;
}
.out .red_number,
.out .red_type,
.out .red_status,
.out .red_text,
.out .red_date,
.out .red_icon
{
    color:#dddbdc
}
.out .red_type{
  border:1px solid #dddbdc
}
.red_number{
  font-size: 35px;
  color:#f6024c
}
.red_icon{
  color:#f6024c;
  font-weight: bold
}
.red_status{
  font-size:12px;
  float:right;
  color:#5f5f5f
}
.red_type{
  display: block;
  color:#fe6464;
  border:1px solid #f2004c;
  text-align: center;
  border-radius:5px;
  float:left;
  margin-left:10px;
  padding:0 5px
}
.red_text{
  margin-left:10px
}
.red_date{
  font-size:1.2em;
  color:#646464;
}
.red_date>li{
  width: 46%;
  float:left;
  height: 40px;
  line-height: 40px;
  padding-left: 10px
}
/*兑换红包弹窗*/
#J_mask{
  background-color: #666;
  position: fixed;
  z-index: 9999;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0.5;
}
.exchange_red{
  background: #fff;
  width: 88%;
  left: 50%;
  margin-left: -44%;
  border-radius:30px;
  position: fixed;
  top:100px;
  z-index: 99999;
  overflow: hidden
}
.exchange_red .exchage_bg{
  background: url("../images/exchange_bg.png");
  background-size:cover;
  width:100%;
  height:50px;
}
.exchange_show{
  display: block
}
.exchange_hide{
  display: none
}
/*兑换内容区*/
.exchange_content .exchange_title{
  width:100%;
  text-align: center;
  color:#fff;
  font-size:14px;
  line-height: 50px
}
.exchange_content .exchange_input{
  width:70%;
  height:35px;
  margin:0 auto;
  margin-top:20px;
  border-radius:10px
}
.exchange_content .exchange_input>input{
  width:100%;
  height:100%;
  padding-left:8px;
  border:1px solid #dcdcdc;
  border-radius:5px
}
.exchange_content .exchange_check>input{
  width:43%;
  height:100%;
  padding-left:8px;
  float:left
}
.exchange_content .exchange_check>img{
  width:28%;
  margin-left:8px
 
}
.exchange_content .exchange_check>span{
  display: block;
  float:right;
  width:22%;
  line-height:18px;
  color:#b9b9b9
}
.exchange_content .exchange_button{
  width:70%;
  margin:0px auto;
  margin-top:20px;
  margin-bottom:25px
}
.exchange_button .btn-danger{
  float:right;
  width:45%;
  color:#fff;
  background: #ff3652;
}
.exchange_button .btn_disable{
  float:left;
  width:45%;
  color:#5f5f5f;
  background: #dddcdd;
}
/*兑换成功*/
.exchange_success .success_title{
  width:100%;
  text-align: center;
  color:#fff;
  font-size:14px;
  line-height: 50px
}   
.exchange_success .success_content{
  text-align: center;
  margin-top: 20px;
  color: #aeaeae;
  line-height: 20px
}
.exchange_success .success_btn{
  text-align: center;
  margin-top: 20px;
  color: #aeaeae;
  margin-bottom:25px;
}
.exchange_success .success_btn>input{
  width:40%;
  background: #ff3652
}
/*验证失败*/
.exchange_fail .fail_title{
  width:100%;
  text-align: center;
  color:#fff;
  font-size:14px;
  line-height: 50px
}
.exchange_fail .fail_content{
  text-align: center;
  margin-top: 20px;
  color: #aeaeae;
  line-height: 20px;
  font-size:15px;
}
.exchange_fail .fail_btn{
  width:70%;
  margin:0px auto;
  margin-top:20px;
  margin-bottom:25px;
}
.exchange_fail .btn-danger{
  float:right;
  width:45%;
  background: #ff3652;
  color:#fff
}
.exchange_fail .btn_disable{
  float:left;
  width:45%;
  background: #dddcdd;
  color:#5f5f5f
}
/*按钮*/
.btn{
  display: inline-block;
  height:36px;
  margin-bottom: 0;
  font-weight: normal;
  text-align: center;
  vertical-align: middle;
  -ms-touch-action: manipulation;
  touch-action: manipulation;
  cursor: pointer;
  background-image: none;
  border: 1px solid transparent;
  white-space: nowrap;
  padding: 3px 12px;
  font-size: 14px;
  line-height: 1.42857143;
  border-radius: 4px;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
