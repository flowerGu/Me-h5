.u_btn_form{
  display:block;
  width:74%;
  margin:0 auto;
  color:@btn-primary-color;
  border-radius:3px;
  padding:.5rem 0;
  text-align:center;
  font-size:16px;
}
.u_btn_form_default{
  .u_btn_form;
  background-color:#ff4c4c;
}
.u_btn_form_disabled{
  .u_btn_form;
  background-color:@btn-disable-color;
}
