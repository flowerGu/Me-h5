/*arrow start*/
.u_arr{
  display:inline-block;
  font-style:normal;
  padding-right:13px;
  position:relative;
  &:after{
    content: " ";
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;
  }
}
.u_arr_right{//右箭头
  .u_arr;
  color: #999999;
  &:after {
    .setArrow(right, 10px, #777576, 1px);
  }
}
.u_arr_left{
  .u_arr;
  color:#000;
  &:after{
    .setArrow(left, 10px, #fff, 1px);
  }
}
/*箭头方向 s*/
.setArrow(@direction, @arrowsize, @borderColor, @borderWidth) when (@direction = top) {
  ._setArrow(@arrowsize, @borderColor, @borderWidth);
  transform: matrix(0.71,-0.71,0.71,0.71,0,0); // rotate(-45deg)
}
.setArrow(@direction, @arrowsize, @borderColor,@borderWidth) when (@direction = right) {
  ._setArrow(@arrowsize, @borderColor, @borderWidth);
  transform: matrix(0.71,0.71,-0.71,0.71,0,0); // rotate(45deg);
  position: relative;
}
.setArrow(@direction, @arrowsize, @borderColor,@borderWidth) when (@direction = down) {
  ._setArrow(@arrowsize, @borderColor, @borderWidth);
  transform: matrix(-0.71,0.71,-0.71,-0.71,0,0); // rotate(135deg);
  position: relative;
}
/*箭头方向 e*/
.setArrow(@direction, @arrowsize, @borderColor,@borderWidth) when (@direction = left) {
  ._setArrow(@arrowsize, @borderColor, @borderWidth);
  transform: matrix(-0.71,-0.71,0.71,-0.71,0,0); // rotate(-135deg);
  position: relative;
  top: -2px;
}
._setArrow(@arrowsize, @borderColor, @borderWidth){
  display: inline-block;
  height: @arrowsize;
  width: @arrowsize;
  border-width: @borderWidth @borderWidth 0 0;
  border-color: @borderColor;
  border-style: solid;
}
/*arrow end*/
/*?*/
.u_ques_mark{
  display:inline-block;
  width:25px;
  height:25px;
  line-height:25px;
  background-color:@pager-bg;
  border-radius:50%;
  color:@brand-danger;
  font-style:normal;
  text-align:center;
  &:after{
    display:inline-block;
    font-size:18px;
    font-weight:bold;
    content:'?';
    color:@brand-danger;
  }
}
/*left arrow*/
.u_back_login{
  display:table;
  position:relative;;
  width:30px;
  height:30px;
  background-color:#dddcdd;
  border-radius:5% 50% 50% 50%;
  font-style:normal;
  transform: matrix(0.71,-0.71,0.71,0.71,0,0);
  >span{
    position:absolute;
    width:14px;
    height:14px;
    left:9px;
    top:9px;
    text-align:center;
    font-size:25px;
    font-weight: bold;
    border-left:2px solid #fff;
    border-bottom:2px solid #fff;
    transform: rotate(90deg);
  }
}
/*form list*/
.u_form_single{
  position:relative;
  height:3rem;
  line-height:3rem;
  background:@btn-primary-color;
  border-bottom:1px solid @nav-tabs-border-color;
  font-size:14px;
  >input[type=text],input[type=tel],input[type=password]{
    position:absolute;
    left:0;
    top:0;
    width:100%;
    height:100%;
    line-height:3rem;
    background:transparent;
  }
}
.u_label_prompt{
  color:#5e5e5e;
  //padding-bottom:1rem;
}
.u_form_single>input.u_input_per{
  width:100%;
  padding-left:80px;
}
.u_form_send{
  position:absolute;
  right:3%;
  top:50%;
  transform:translate(-20%,-50%);
  background:transparent;
  border-left:1px solid #e9e9e9;
  padding:.5rem 0 .5rem .8rem;
  color:#ff4c4c;
  border-radius:0
}
