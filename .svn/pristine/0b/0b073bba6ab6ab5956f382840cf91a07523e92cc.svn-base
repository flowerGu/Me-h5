/*---reset----*/
div,a,b,input,textarea,button,select,form,label,span,td,li,time,body,ul,i,h1,h2,h3,p,table{padding:0;margin:0;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;-ms-box-sizing:border-box;word-wrap:break-word;word-break:break-all;-webkit-tap-highlight-color:rgba(0,0,0,0)}
article,aside,footer,header,section,nav,fieldset{display:block;padding:0;margin:0}
ul,ol,li {list-style:none}
input,img,fieldset{border:none}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {-webkit-appearance: none !important;margin: 0;}
img{vertical-align:middle}
table,td{cellpadding:0;cellspacing:0;border-collapse:collapse;border-spacing:0}
.clearfix:after {content:".";display:block;height:0;clear:both;visibility:hidden}
.clearfix {display:inline-block}
.clearfix {display:block}
a{text-decoration:none;-webkit-tap-highlight-color: rgba(255, 255, 255, 0);}
a:focus,input{outline:none}/*for Firfox*/
b,strong{font-weight:normal}
h1,h2{font-weight:normal}
body{margin:0 auto;background:#fff;width:100%;min-width:320px;font-size:82%;color:#5F5F5F;font-family: STHeitiSC Light,"微软雅黑"}
@media screen and (max-width:480px){body{font-size:65%}}
@media screen and (min-width:480px) and (max-width:960px){body{font-size:86%}}
@media screen and (min-width:960px){body{font-size:86%}}
/*------float----*/
.fl{float:left}
.fr{float:right}
/*-----color-----*/
.s_defult{color:#ff4c4d}
.s_gray{color:#8f8f8f}
.s_lightGray{color:#e9e9e9}
.s_fff{color:#fff}
.s_000{color:#000}
.s_darkRed{color:#ea4645}