	<script>
		var date = new Date();
		var year = date.getFullYear();
		var month = date.getMonth() + 1;
		var date = date.getDate();
		var currentTime = this.year + "年" + this.month + "月" + this.date + "日 " ;
		console.log(currentTime)
		document.getElementById("jianxiu_time").innerHTML = currentTime;
