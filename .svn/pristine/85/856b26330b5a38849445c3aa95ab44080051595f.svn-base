<body class="has-nav">
  <div id="dropload">
    <!---->
    <div class="swiper-container">
      <div class="swiper-wrapper">
        <script type="text/html" id="banner">
          {{#banner_urls}}
          <div class="swiper-slide"><a href="{{banner_url}}"><img src="{{pic_url}}" alt="{{title}}"></a></div>
          {{/banner_urls}}
        </script>
      </div>
      <div class="swiper-pagination"></div>
    </div>
    <!---->
    <ul class="lend-statistics" id="statistics"></ul>
    <script type="text/html" id="statistics_tpl">
      <li><span>累计出借：{{dealAmount}}</span></li>
      <li><span>累计注册：{{tradeNum}}</span></li>
    </script>
    <!--图标按钮-->
    <ul class="icons-menu" id="icon_menu"></ul>
    <script type="text/html" id="icon_menu_tpl">
      {{#banner_urls}}
        <li>
          <a href="{{banner_url}}">
            <img src="{{pic_url}}" alt="{{title}}">
            <p>{{title}}</p>
          </a>
        </li>
      {{/banner_urls}}
    </script>
    <!---->
    <div class="list-gourp">

      <div class="list-gourp-item switch-new-me-business">
        <div class="list-gourp-header">
          <span>新手标</span>
          <!--<a href="me.html?type=me" class="link-more">更多<i class="icon-arrow"></i></a>-->
        </div>
        <div class="list-gourp-body" id="new_prod_list">
          <script type="text/html" id="new_prod_list_tpl">
            {{#newProducts}}
              <a class="list-item {{#status2}} disabled{{/status2}}" 
                data-id="{{id}}"
                data-status="{{status}}"
                data-investment="{{pre_investment}}"
                data-product-type="{{product_type_name}}">
                  <div class="list-item-header">
                    <h1>{{product_name}}</h1>
                    <i class="product-type">{{product_type_name}}</i>
                  </div>
                  <div class="list-item-body">
                    <div class="row-1">
                      <span class="list-item-1">{{newYears_income}}<em class="list-item-rate-unit">%</em></span>
                      <span class="list-item-2"><span class="list-item-2-num">{{period}}</span><span class="list-item-2-unit">个月</span></span>
                      <span class="btn pull-right {{#status1}}btn-danger{{/status1}}{{#status2}}btn-disable{{/status2}}">{{#status1}}出借{{/status1}}{{#status2}}售罄{{/status2}}</span>
                    </div>
                    <div class="process-bar">
                      <div class="process-bar-val" style="width:{{progress}}%"></div>
                    </div>
                    <div class="row-3">
                      <span>{{start_invest_money}}元起借</span>
                      <span class="describe">/{{describe}}</span>
                      <span class="pull-right">{{surplusInvestMoney}}元/{{totalMoney}}元</span>
                    </div>
                  </div>
                </a>
            {{/newProducts}}
          </script>
        </div>
      </div>

