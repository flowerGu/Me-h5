<div class="detail_con border_tb whitebg">
			<div class="detail_list btm">
				<label><i>＊</i>客户姓名</label>
				<span>{{#girl}}<img src="./images/girl.png">{{/girl}}{{#boy}}<img src="./images/boy.png">{{/boy}}{{custname}}</span>
			</div>
			<div class="detail_list">
				<label><i>＊</i>手机号</label>
				<span>{{phonenums}}</span>
			</div>
		</div>
		
		<div class="detail_con border_tb whitebg">
			<div class="detail_list btm">
				<label>客户来源</label>
				<span>{{custsource}}</span>
			</div>
			<div class="detail_list btm">
				<label>证件类型</label>
				{{#idtypecd1}}
					<span>身份证</span>
				{{/idtypecd1}}
				{{#idtypecd2}}
					<span>其他</span>
				{{/idtypecd2}}
			</div>
			<div class="detail_list btm">
				<label>证件号码</label>
				<span>{{idnumber}}</span>
			</div>
			<div class="detail_list btm">
				<label>客户等级</label>
				<span>{{level}}{{^level_type}}级{{/level_type}}</span>
			</div>
			<div class="detail_list btm">
				<label>年龄</label>
				<span>{{custage}}</span>
			</div>
			<div class="detail_list btm">
				<label>固定电话</label>
				<span>{{telnums}}</span>
			</div>
			<div class="detail_list btm">
				<label>通讯地址</label>
				<span>{{contactaddress}}</span>
			</div>
			<div class="detail_list" style='height: 100px;'>
				<label>备注</label>
				
				<span style='left:10%;line-height: 1.5em;margin-top: 10px;'>{{remark}}</span>
			</div>
		</div>
